#---------------------------------------
# Localized Forms
#---------------------------------------
feature locl { # Localized Forms
	script latn;
		language MOL  exclude_dflt; # Moldavian
			sub [Scedilla scedilla Tcommaaccent tcommaaccent] by [Scommaaccent scommaaccent uni021A uni021B];
		language ROM  exclude_dflt; # Romanian
			sub [Scedilla scedilla Tcommaaccent tcommaaccent] by [Scommaaccent scommaaccent uni021A uni021B];
		language TRK exclude_dflt;
			sub i by i.TRK;
		language AZE exclude_dflt;
			sub i by i.TRK;
		language CRT exclude_dflt;
			sub i by i.TRK;
} locl;




#---------------------------------------
# Fractions
#---------------------------------------

# Tal Leming FRAC code
# 1 OCR 2009
# http://talleming.com/2009/10/01/fraction-fever-2/
feature frac {
   lookup FractionBar {
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures @figures slash;
        ignore sub slash @figures @figures @figures slash';
        ignore sub slash' @figures @figures @figures slash;
        ignore sub slash @figures @figures slash';
        ignore sub slash' @figures @figures slash;
        ignore sub slash @figures slash';
        ignore sub slash' @figures slash;
        sub @figures slash' @figures by fraction;
   } FractionBar;

   lookup Numerator1 {
        sub @figures' fraction by @numerators;
   } Numerator1;

   lookup Numerator2 {
        sub @figures' @numerators fraction by @numerators;
   } Numerator2;

   lookup Numerator3 {
        sub @figures' @numerators @numerators fraction by @numerators;
   } Numerator3;

   lookup Numerator4 {
        sub @figures' @numerators @numerators @numerators fraction by @numerators;
   } Numerator4;

   lookup Numerator5 {
        sub @figures' @numerators @numerators @numerators @numerators fraction by @numerators;
   } Numerator5;

   lookup Numerator6 {
        sub @figures' @numerators @numerators @numerators @numerators @numerators fraction by @numerators;
   } Numerator6;

   lookup Numerator7 {
        sub @figures' @numerators @numerators @numerators @numerators @numerators @numerators fraction by @numerators;
   } Numerator7;

   lookup Numerator8 {
        sub @figures' @numerators @numerators @numerators @numerators @numerators @numerators @numerators fraction by @numerators;
   } Numerator8;

   lookup Numerator9 {
        sub @figures' @numerators @numerators @numerators @numerators @numerators @numerators @numerators @numerators fraction by @numerators;
   } Numerator9;

   lookup Numerator10 {
        sub @figures' @numerators @numerators @numerators @numerators @numerators @numerators @numerators @numerators @numerators fraction by @numerators;
   } Numerator10;

   lookup Denominator {
        sub [fraction @denominators] @figures' by @denominators;
   } Denominator;

   sub @figures space' @numerators by uni200A;

} frac;


#---------------------------------------
# Small Capitals From Capitals
#---------------------------------------

feature c2sc {
	sub @CAPITALS_HAVING_SC by @CAPITALS_SC;
	sub @SC_HAVING_EXTRAS by @SC_EXTRAS;
	sub @FIG_HAVING_SMCP by @FIG_SMCP;
	#capital eszett
	sub uni1E9E by uni1E9E.c2sc;		
} c2sc;


#---------------------------------------
# Small Capitals
#---------------------------------------

feature smcp {

		lookup SMCP_ALL {
			sub @LOWER_HAVING_SC by @LOWER_SC;
			sub dotlessi by i.smcp;
			sub @FIG_TNUM_LNUM by @FIG_TNUM_ONUM;
			sub @FIG_DEFAULT by @FIG_PNUM_ONUM;
			sub @CURRENCY_DEFAULT by @CURRENCY_PNUM_ONUM;
			sub zero.zero by zero.pnum_onum_zero;
			sub zero.tnum_onum_zero by zero.tnum_lnum;
			} SMCP_ALL;
			lookup SMCP_ITRK {
				sub i.TRK by idotaccent.smcp; 
				
			} SMCP_ITRK;
} smcp;



#---------------------------------------
# Discretionary Ligatures
#---------------------------------------
feature ordn {
    sub @FIGURES a' by ordfeminine;
    sub @FIGURES o' by ordmasculine;
    sub @FIGURES period a' by ordfeminine;
    sub @FIGURES period o' by ordmasculine;
    sub @ONE s' t by s.superior;
    sub @ONE s.superior t' by t.superior;
    sub @TWO n' d by n.superior;
    sub @TWO n.superior d' by d.superior;
    sub @THREE r' d by r.superior;
    sub @THREE r.superior d' by d.superior;
    sub @FIGURES t' h by t.superior;
    sub @FIGURES t.superior h' by h.superior; 
    sub [N n] [O o] by uni2116;
} ordn;





#---------------------------------------
# Oldstyle Figures
#---------------------------------------
feature onum {
	sub @FIG_DEFAULT by @FIG_PNUM_ONUM;
	sub @CURRENCY_DEFAULT by @CURRENCY_PNUM_ONUM; 
	sub @FIG_TNUM_LNUM by @FIG_TNUM_ONUM; 
    sub @PUNCT_CAP by @PUNCT_DEFAULT; 
	sub zero.zero by zero.pnum_onum_zero;
} onum;



	

#---------------------------------------
# Lining Figures
#---------------------------------------
feature lnum {
    
	sub @FIG_PNUM_ONUM by @FIG_DEFAULT; 	
    sub @FIG_TNUM_ONUM by @FIG_TNUM_LNUM;    
	sub @CURRENCY_PNUM_ONUM by @CURRENCY_DEFAULT;
	sub @PUNCT_DEFAULT by @PUNCT_CAP;  
    sub zero.pnum_onum_zero by zero.zero;
} lnum;

	
	

#---------------------------------------
# Proportional Figures
#---------------------------------------
feature pnum {
    sub @FIG_TNUM_ONUM by @FIG_PNUM_ONUM;
    sub @FIG_TNUM_LNUM by @FIG_DEFAULT;
} pnum;




#---------------------------------------
# Tabular Figures
#---------------------------------------
feature tnum {
		sub @FIG_DEFAULT by @FIG_TNUM_LNUM;
		sub @FIG_PNUM_ONUM by @FIG_TNUM_ONUM;
} tnum;




feature sups {

	script latn;
		language dflt;
			lookup SUPS_DFLT {
				sub i by i.superior;
			} SUPS_DFLT;

			lookup SUPS_ALL {
				sub @FIG_DEFAULT by @FIG_SUPERIOR;
				sub @FIG_NUMR by @FIG_SUPERIOR;
				sub @FIG_TNUM_LNUM by @FIG_SUPERIOR;
				sub @FIG_PNUM_ONUM by @FIG_SUPERIOR;
				sub @FIG_TNUM_ONUM by @FIG_SUPERIOR;
				sub @FIG_INFERIOR by @FIG_SUPERIOR;
				sub [zero.zero zero.pnum_onum_zero] by zerosuperior;
				sub @LETTERS_HAVING_SUPERIOR by @LETTERS_SUPERIOR;
				sub @PUNCT_HAVING_SUPERIOR by @PUNCT_SUPERIOR;
				sub @PUNCT_INFERIOR by @PUNCT_SUPERIOR;
				sub parenleft.cap by parenleftsuperior;
				sub parenright.cap by parenrightsuperior;
				sub [hyphen hyphen.cap] by minussuperior;
			} SUPS_ALL;
			
		language DEU include_dflt;
		language MOL include_dflt;
		language ROM include_dflt;
		language AZE include_dflt;
		language CRT include_dflt;
		language TRK include_dflt;
} sups;





#---------------------------------------
# Scientific Inferiors
#---------------------------------------
feature sinf {
	
	script latn;
		language dflt;
			lookup SINF_DFLT {
				sub i by i.inferior;
			} SINF_DFLT;
	
			lookup SINF_ALL {	sub @FIG_DEFAULT by @FIG_INFERIOR;
				sub @FIG_TNUM_LNUM by @FIG_INFERIOR;
				sub @FIG_PNUM_ONUM by @FIG_INFERIOR;
				sub @FIG_TNUM_ONUM by @FIG_INFERIOR;
				sub @FIG_INFERIOR by @FIG_INFERIOR;
				sub [zero.zero zero.pnum_onum_zero] by zeroinferior;
				sub @LETTERS_HAVING_INFERIOR by @LETTERS_INFERIOR;
				sub @PUNCT_HAVING_INFERIOR by @PUNCT_INFERIOR;
				sub @PUNCT_SUPERIOR by @PUNCT_INFERIOR;
				sub parenleft.cap by parenleft.inferior;
				sub parenright.cap by parenright.inferior;
				sub [hyphen hyphen.cap] by minus.inferior;
			} SINF_ALL;
			
		language DEU include_dflt;
		language MOL include_dflt;
		language ROM include_dflt;
		language AZE include_dflt;
		language CRT include_dflt;
		language TRK include_dflt;
} sinf;





